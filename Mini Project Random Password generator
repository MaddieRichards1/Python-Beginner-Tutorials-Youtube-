# Importing the relevant modules import random
from random import randint

# All uppercase password
password = ""
for i in range(10):
    i = chr(randint(65, 90)) # characters in randint are a-z
    password = str(password)+i # the string is the password plus i (random character)
print(password)

# Upper and lower case password
password = ""
for i in range(5): # repeats loop 5 times
    i = chr(randint(65, 90))
    for j in range(5):
        j = chr(randint(65, 90)) .lower()
    password = str(password) + i + j
print(password)
